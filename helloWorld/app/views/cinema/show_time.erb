<%= stylesheet_link_tag 'show_time.css' %>
<table>
  <th>Movie</th>
  <th>Time</th>
  <% @show_times.each { |movie| %>
      <tr>
        <td>
          <%= movie[0] %>
        </td>
        <td>
          <%= movie[1].join(', ') %>
        </td>
      </tr>

  <% }%>

</table>
<%= button_to 'Back To Home', cinema_index_path, :method => :post %>
